<%- include('partials/head')%>


    <body>
        <%- include('partials/navbar') %>

		<section class="live-products-section">
            <h2>Live auction Products</h2>
            
            <div class="container2">
                
                <% for(let product of products) { %>
                    <% if(product.isLive != false && isCompleted == false){ %> 
                        <div class="square-blocks">
                            <img src="/images/test">
                            <h3 class="product-name"><%= product.name %></h3>
                            <p ><%= product.endDate %></p>
                            <p ><%= product.price %></p>
                        </div>
                        <% } %> 
                <% } %>
            </div>
        </section>

		<section class="Upcomming-products-section">
            <h2>Upcomming auction Products</h2>
            
            <div class="container2">
                <% for(let product of products) { %>
                    <% if(product.isLive == false && isCompleted == false){ %> 
                        <div class="square-blocks">
                            <img src="/images/test">
                            <h3 class="product-name"><%= product.name %></h3>
                            <p ><%= product.startDate %></p>
                            <p ><%= product.price %></p>
                        </div>
                        <% } %> 
                <% } %>
            </div>
        </section>



	</body>

<%- include('partials/foot')%>